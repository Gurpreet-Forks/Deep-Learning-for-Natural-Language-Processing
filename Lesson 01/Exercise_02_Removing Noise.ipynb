        "import re\n",
        "def clean_words(text):\n",
        "  text = re.sub(\"(<.*?>)\",\"\",text)\n",
        "  text=re.sub(\"(\\W|\\d+)\",\" \",text)\n",
        "  text=text.strip()\n",
        "  \n",
        "  return text"
      ],
      "execution_count": 0,
      "outputs": []
    },
